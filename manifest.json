{
    "manifest_version": 3,
    "name": "newtab",
    "version": "1.0",
    "description": ":)",
    
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },

    "permissions": [
        "storage"
    ],

    "icons": {
        "128": "icon.png"
    },

    "options_page": "options.html",
    "options_ui": {
        "page": "options.html",
        "open_in_tab": true
    }
}
